{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src/"],
    "exclude": ["dist", "node_modules", "tests"],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["node_modules/jsdoc-vue"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "recurseDepth": 10,
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./jsdoc/",
    "recurse": true,
    "tutorials": "examples"
  }
}
