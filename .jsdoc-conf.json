{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src/*"],
    "exclude": ["dist", "node_modules", "tests"],
    "includePattern": "\\.(vue|js)$"
  },
  "plugins": ["node_modules/jsdoc-vue"],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
  }
}
